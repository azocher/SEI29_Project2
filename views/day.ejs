<% meals.forEach(meal => {%>
    <% if(meal){%>
        <h1>
            <%= meal.day%>
        </h1>
        <div>
            <h3>
                <%= meal.time%>
            </h3>
        </div>

        <div>
            <img src="<%= meal.image%>">
        </div>

        <div>
            <h4>
                <%= meal.name%>
            </h4>

        </div>
        <%}else{%>
            <h2>No meal selected this day</h2>
            <%}%>
                <% })%>